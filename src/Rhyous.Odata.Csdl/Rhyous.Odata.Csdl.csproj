<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <Authors>Jared Barneck (Rhyous)</Authors>
    <Company>Rhyous Publishing LLC</Company>
    <Version>1.2.9</Version>
    <AssemblyVersion>1.2.9.0</AssemblyVersion>
    <FileVersion>1.2.9.0</FileVersion>
    <Copyright>Copyright Jared Barneck (Rhyous) 2017</Copyright>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <Description>A library to aid in providing csdl format for a given type.</Description>
    <PackageTags>csdl</PackageTags>
    <PackageProjectUrl>https://github.com/rhyous/Odata</PackageProjectUrl>
    <RepositoryUrl>https://github.com/rhyous/Odata</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Rhyous.Collections" Version="1.2.8" />
    <PackageReference Include="Rhyous.StringLibrary" Version="1.2.17" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Rhyous.Odata\Rhyous.Odata.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
